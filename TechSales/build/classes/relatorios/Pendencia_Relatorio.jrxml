    <queryString>
		<![CDATA[SELECT 
    cliCodigo, cliNome, joNome, devDataDevolucao
FROM
    cliente
        JOIN
	jogo
		JOIN	
    locacao
        JOIN
    devolucao
WHERE
	TIMESTAMPDIFF(DAY, loDataAluguel, devDataDevolucao) >= 7
    AND loc_cliCodigo = cliCodigo
        AND joCodigo = loc_joCodigo
        AND dev_loCodigo = loCodigo
        AND cliente_cliCodigo = cliCodigo
       
GROUP BY cliCodigo]]>
    </queryString>
